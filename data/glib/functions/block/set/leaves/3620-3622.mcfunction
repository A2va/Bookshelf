execute if score @s glib.blockId matches 3620 run setblock ~ ~ ~ jungle_wood[east=false,north=false,south=false,waterlogged=false,west=false,axis=x]
execute if score @s glib.blockId matches 3621 run setblock ~ ~ ~ jungle_wood[axis=y]
execute if score @s glib.blockId matches 3622 run setblock ~ ~ ~ tall_seagrass[axis=z,half=upper]
